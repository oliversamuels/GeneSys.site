@inherits UmbracoViewPage<ContentModels.CaseStudy>
@{
	Layout = "Master.cshtml";
	ViewData["Title"] = Model.Title;
}

<div class="container p-0">
    <div class="newshead">
        <div class="container-fluid px-0">
            <div>
                <h2 class="text-center p-0 m-0">Our Product Case Study</h2>
