@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GeneSys Health</title>
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,300&display=swap" rel="stylesheet">
    <link href="/css/site.css" rel="stylesheet">
    <link href="/css/AboutUs.css" rel="stylesheet">
    <link href="/css/News.css" rel="stylesheet">
    <link href="/css/How.css" rel="stylesheet">
    <link href="/css/Demo.css" rel="stylesheet">
    <link href="/css/products.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/00ea422355.js" crossorigin="anonymous"></script>
</head>
<body>
    <header class="container-fluid  p-0">
        <nav class="navbar navbar-expand-lg bg-gradient">
            <div class="container-fluid px-5">
                <a class="navbar-brand" href="/"><img src="~/assets/images/GHIS_logo.png" alt="GHIS Logo"/></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupport" aria-controls="navbarSupport" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupport">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0 m-auto">
                        <li class="nav-item dropdown bg-transparent">
                            <a class="nav-link dropdown-toggle fw-bold" role="button" data-bs-toggle="dropdown" aria-expanded="false" asp-area="" asp-page="">Product</a>
                            <ul class="dropdown-menu dropdown-menu-transparent bg-gradient">
                                <li><a class="dropdown-item" href="/products">Product 1</a></li>
                                <li><a class="dropdown-item" href="/products">Product 2</a></li>
                                <li><hr class="dropdown-divider"></li>
                                <li><a class="dropdown-item" href="/products">Product 3</a></li>
                                <li><a class="dropdown-item" href="/products">Product 4</a></li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link fw-bold" href="/about">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link fw-bold" href="/news">News</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link fw-bold" href="/how">How it Works</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link fw-bold" href="/contact">Contact</a>
                        </li>
                    </ul>
                    <ul class="navbar-nav">
                        <li><a class="nav-link navb btn btn-primary p-2" href="/demo">Request Demo</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid section">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <script src="/scripts/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>